<% include partials/header %>


	<div class="container">
			<form action="/admins/login" class="col-md-6 offset-md-3" method="POST">
			<legend align="center"><i class="fas fa-user-secret"></i> Admin Login</legend>
				<div class="form-group">
				<label for="username">User Name</label>
				<input class="form-control" type="text" name="username" placeholder="EX:Admin" required="" autofocus>
				</div>
				<div class="form-group">
				<label for="password">Password</label>
				<input class="form-control" type="password" name="password" required="" autofocus>
				</div>
				<div class="form-group">
				<button class="btn btn-primary">LogIn</button>
				<a class="float-right" href="/homes">Go Back!</a>
				</div>
			</form>
	</div>
<% include partials/footer %>